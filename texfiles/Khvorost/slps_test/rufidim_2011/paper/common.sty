\newcommand{\ComputingFiValue}{
    \pic{0}{-0.75}{10}{4.5}{
        \textline{1.5}{4}{$result = +\infty;$}
        \textline{1.5}{3.5}{$l = 0, r = |f_{i+p}| + \dots + |f_{i+q}|;$}
        \textline{1.5}{3}{\textbf{for} ($int~r = p; r \leq q; r++;$) \{ }
        	\textline{2}{2.5}{$l += |f_{i+r}|;$}
        	\textline{2}{2}{$r -= |f_{i+r}|;$}
        	\textline{2}{1.5}{$tmp = \varphi(p,r) + \varphi(r+1, q) + |\log l - \log r|;$}
            \textline{2}{1}{\textbf{if} ($tmp < result$)}
        		\textline{2.5}{0.5}{$result = tmp;$}
        \textline{1.5}{0}{\}}
        \textline{1}{-0.75}{{\bf Figure \arabic{figures}.} Pseudo code of computing single value of $\varphi(p,q)$}
    }
}

